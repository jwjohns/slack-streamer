{
  "name": "slack-streamer",
  "version": "0.0.1",
  "description": "Stream text updates into Slack with throttling and session modes.",
  "license": "MIT",
  "author": "Justin Johns",
  "keywords": [
    "slack",
    "streaming",
    "chat",
    "rate-limit"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "test": "vitest"
  },
  "peerDependencies": {
    "@slack/web-api": ">=6"
  },
  "peerDependenciesMeta": {
    "@slack/web-api": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "typescript": "^5.6.3",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=18"
  }
}
